{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/your-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/your-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/your-app",
  "projects": {
    "your-app": {
      "type": "application",
      "root": "apps/your-app",
      "entryFile": "main",
      "sourceRoot": "apps/your-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/your-app/tsconfig.app.json"
      }
    },
    "messages": {
      "type": "application",
      "root": "apps/messages",
      "entryFile": "main",
      "sourceRoot": "apps/messages/src",
      "compilerOptions": {
        "tsConfigPath": "apps/messages/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "profile": {
      "type": "application",
      "root": "apps/profile",
      "entryFile": "main",
      "sourceRoot": "apps/profile/src",
      "compilerOptions": {
        "tsConfigPath": "apps/profile/tsconfig.app.json"
      }
    },
    "libs": {
      "type": "library",
      "root": "libs/libs",
      "entryFile": "index",
      "sourceRoot": "libs/libs/src",
      "compilerOptions": {
        "tsConfigPath": "libs/libs/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}